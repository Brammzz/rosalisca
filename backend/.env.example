# Environment Variables Template
# Copy this to .env and fill in your values

# Database
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/rosaliscaDB?retryWrites=true&w=majority

# JWT Secret (minimum 32 characters)
JWT_SECRET=your-super-secret-jwt-key-that-is-long-and-random-for-production-security

# Port (optional, Vercel will set automatically)
PORT=5000

# Environment
NODE_ENV=production
